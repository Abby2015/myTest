<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yum.kfc.brand.luh1605.dao.Luh1605DrawMapper">

    <select id="queryIsUserDraw" resultType="Long">
		select count(*) from luh1605_draw with(nolock) where phone=#{phone} 
	</select>
	

    
    <insert id="insert" parameterType="com.yum.kfc.brand.luh1605.pojo.Luh1605Draw">
    	INSERT INTO luh1605_draw
    		([phone],[openId],[wxOpenId],[userId],[channelType],[deviceType],[deviceId],[ibeaconSN],[winAward],[awardType],[awardCodeId],[ipAddr],[drawTime])
     	VALUES
     		 (#{phone},#{openId},#{wxOpenId},#{userId},#{channelType},#{deviceType},#{deviceId},#{ibeaconSN},#{winAward},#{awardType},#{awardCodeId},#{ipAddr},#{drawTime})
    </insert>

    
</mapper>