<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yum.kfc.brand.xrbj1506.dao.Xrbj1506OpenMapper">
	<insert id="insert" parameterType="com.yum.kfc.brand.xrbj1506.pojo.Xrbj1506Open">
		INSERT INTO xrbj1506_open
			([id],[userId],[inviterId],[menuType],[channelType],[deviceId],[deviceType],[token],[inviteChannelType],[tagContent],[ipAddr],[openTime])
		VALUES
			(#{id},#{userId},#{inviterId},#{menuType},#{channelType},#{deviceId},#{deviceType},#{token},#{inviteChannelType},#{tagContent},#{ipAddr},#{openTime})
	</insert>
	
	<select id="getTagContent" parameterType="String" resultType="String">
		select tagContent from xrbj1506_open with(nolock) where id=#{openId}
    </select>
</mapper>